GEMINI_API_KEY=your_gemini_api_key_here
PORT=8000

# Google Cloud API Configuration
# Option 1: Use API Key (Fallback when user not connected)
GOOGLE_CLOUD_API_KEY=your_google_cloud_api_key_here

# Option 2: Use Service Account JSON (recommended for production)
# GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account-key.json

# Google Cloud Service Port
GOOGLE_CLOUD_PORT=8002

# Google OAuth 2.0 Configuration (for user-based quota)
GOOGLE_OAUTH_CLIENT_ID=your_client_id.apps.googleusercontent.com
GOOGLE_OAUTH_CLIENT_SECRET=your_client_secret
GOOGLE_OAUTH_REDIRECT_URI=http://localhost:8080/api/auth/google/callback

# OAuth Service Configuration
OAUTH_SERVICE_PORT=8003
OAUTH_SERVICE_URL=http://localhost:8003
GOOGLE_CLOUD_OAUTH_PORT=8004

# Frontend URL
FRONTEND_URL=http://localhost:3000

# Spring Boot URL
SPRING_BOOT_URL=http://localhost:8080

# Encryption key for storing OAuth tokens (generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())")
ENCRYPTION_KEY=your_encryption_key_here
